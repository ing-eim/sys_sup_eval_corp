<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>UploadFiles</servlet-name>
        <servlet-class>pbi.supeval.servlets.UploadFiles</servlet-class>
        <multipart-config>
            <location>/tmp</location>
            <max-file-size>20848820</max-file-size>
            <max-request-size>418018841</max-request-size>
            <file-size-threshold>1048576</file-size-threshold>
        </multipart-config>
    </servlet>
    <servlet>
        <servlet-name>LoadQueja</servlet-name>
        <servlet-class>pbi.supeval.servlets.LoadQueja</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadEstimulos</servlet-name>
        <servlet-class>pbi.supeval.servlets.LoadEstimulos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadObservacion</servlet-name>
        <servlet-class>pbi.supeval.servlets.LoadObservacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchObservacion</servlet-name>
        <servlet-class>pbi.supeval.servlets.SearchObservacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateObservacion</servlet-name>
        <servlet-class>pbi.supeval.servlets.UpdateObservacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteObservacion</servlet-name>
        <servlet-class>pbi.supeval.servlets.DeleteObservacion</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadFiles</servlet-name>
        <url-pattern>/view/UploadFiles</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadQueja</servlet-name>
        <url-pattern>/view/LoadQueja</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadEstimulos</servlet-name>
        <url-pattern>/view/LoadEstimulos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadObservacion</servlet-name>
        <url-pattern>/view/LoadObservacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchObservacion</servlet-name>
        <url-pattern>/view/SearchObservacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateObservacion</servlet-name>
        <url-pattern>/view/UpdateObservacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteObservacion</servlet-name>
        <url-pattern>/view/DeleteObservacion</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
